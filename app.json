{
  "expo": {
    "name": "Marine Business",
    "slug": "expomb",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/main.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dedislamets.expo.mb"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/launcher.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dedislamets.expo.mb"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font"
      ],
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ]
    ],
    "extra": {
      "oneSignalAppId": "5177015b-39ce-4219-bcbb-b14588ea6b74",
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "OneSignalNotificationServiceExtension",
                  "bundleIdentifier": "com.dedislamets.expo.mb.OneSignalNotificationServiceExtension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.dedislamets.expo.mb.onesignal"
                    ]
                  }
                }
              ]
            }
          }
        },
        "projectId": "7a35a2af-d4a9-47a5-bf3b-7177a7101233"
      }
    }
  }
}
